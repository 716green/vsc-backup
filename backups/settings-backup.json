{
  "editor.tabSize": 2,
  "editor.fontSize": 15,
  "editor.suggestSelection": "first",
  "editor.fontFamily": "Fira Code",
  "editor.codeLensFontFamily": "Fira Code",
  "editor.formatOnSave": true,
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Fira Code, MesloLGS NF, Hack Nerd Font",
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 2000,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.letterSpacing": 1,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "go.toolsGopath": "/Users/bob/go/",
  "go.gopath": "/Users/bob/go/",
  "go.useLanguageServer": true,
  "go.formatTool": "gofmt",
  "go.toolsManagement.autoUpdate": true,
  "html.format.wrapLineLength": 0,
  "javascript.preferences.quoteStyle": "single",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.convertOutermostQuotes": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "prettier.singleQuote": true,
  "prettier.vueIndentScriptAndStyle": true,
  "prettier.jsxBracketSameLine": true,
  "vetur.experimental.templateInterpolationService": false,
  "vetur.ignoreProjectWarning": true,
  "vetur.validation.templateProps": true,
  "emmet.includeLanguages": {
    "vue": "html",
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "files.associations": {
    "*.tsx": "typescriptreact"
  },
  "C_Cpp.updateChannel": "Insiders",
  "explorer.confirmDelete": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "material-icon-theme.activeIconPack": "none",
  "path-intellisense.showHiddenFiles": true,
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "vsicons.dontShowNewVersionMessage": true,
  "python.pythonPath": "/usr/local/bin/python",
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "explorer.confirmDragAndDrop": false,
  "html-css-class-completion.enableEmmetSupport": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSaveMode": "modifications"

}
